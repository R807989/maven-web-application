pipeline
{
  agent any
environment
{
PATH="/var/lib/jenkins/tools/hudson.tasks.Maven_MavenInstallation/maven/bin:$PATH"

}
  stages
  {
    stage("intigration") {
      steps{
      git 'https://github.com/R807989/maven-web-application.git'
    }
  
}
 
  
    stage("build") {
      steps{
      sh "mvn clean install package"
    }
  
}

stage("test") {
      steps{
      withSonarQubeEnv("sonarqbe") {
     sh "mvn  sonar:soanr"
    }
  
}
}
}
}
