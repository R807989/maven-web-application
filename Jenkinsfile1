pipeline {

agent any

tools{
maven "maven"

}
stages{


stage ("git clone"){


steps{

git 'https://github.com/R807989/maven-web-application.git'
}
}

stage ("build"){


steps{

sh "mvn clean install package"


}


}
stage ("code analysis"){


steps{

withSonarqubeENV("sonar"){
sh "mvn sonar:sonar"
}
}

}
}

}
