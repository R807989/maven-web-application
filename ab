pipeline {
agent any 
tools{
maven "maven-9"
}
stages{
stage("git")
  {
 
    steps {
       git 'https://github.com/R807989/maven-web-application.git'
    }
    }


stage("build")
  {
 
    steps {
       sh "mvn clean install package"
    }
    }
stage("test") {
steps {
withsonarQubeEnv("son"){
sh "mvn sonar:sonar"
}
}
}

}
  
}
